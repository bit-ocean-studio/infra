{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Nest.js",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2021",
    "declaration": true,
    "removeComments": true,
    "incremental": true,
    "strictPropertyInitialization": false,
    "useDefineForClassFields": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
