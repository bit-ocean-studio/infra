{
  "name": "docs",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
