    "prettier:check": "prettier --check --cache --ignore-unknown --ignore-path=../../.prettierignore .",
    "prettier:fix": "prettier --write --cache --ignore-unknown --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "@astrojs/starlight": "^0.25.4",
    "astro": "^4.13.2",
    "sharp": "^0.33.4",
